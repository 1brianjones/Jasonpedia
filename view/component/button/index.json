{
  "$jason": {
    "head": {
      "title": "Buttons",
      "data": {
        "items": [
          { "type": "Rectangle button", "url": "1.json" },
          { "type": "Rounded button", "url": "2.json" }
        ]
      },
      "templates": {
        "body": {
          "sections": [{
            "items": {
              "{{#each items}}": {
                "type": "label",
                "style": {
                  "font": "HelveticaNeue-Bold",
                  "size": "15"
                },
                "href": {
                  "url": "{{url}}"
                },
                "text": "{{type}}"
              }
            }]
          }]
        }
      }
    }
  }
}
